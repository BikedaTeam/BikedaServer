<!DOCTYPE html>
<html>
<%- include('header') %>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">
  <%- include('topMenu') %>
  <%- include('sideMenu') %>
  <div class="content-wrapper">
    <section class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1>상점 관리</h1>
          </div>
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="/branch/main/realTimeDelivery">Home</a></li>
              <li class="breadcrumb-item active">상점 관리</li>
            </ol>
          </div>
        </div>
      </div><!-- /.container-fluid -->
    </section>
    <!-- Main content -->
    <section class="content">
      <!-- 배달 현황 -->
      <div class="container-fluid">
        <div class="row">
          <div class="col-12">
            <div class="card">
              <div class="card-body">
                <form class="form" action="#" method="post">
                  <div class="row">
                    <div class="input-group col-sm-6 col-9">
                      <div class="input-group-prepend">
                        <button type="button" class="btn btn-nomal disabled">상호</button>
                      </div>
                      <input type="text" class="form-control">
                    </div>
                    <div class="input-group col-md-1 offset-md-5 col-sm-2 offset-sm-4 col-3">
                      <button type="button" class="form-control btn btn-primary">검색</button>
                    </div>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
        <div class="row d-none d-lg-block">
          <div class="col-12">
            <div class="card">
              <div class="card-body">
                <table id="tb_store" class="table table-bordered table-hover">
                  <thead>
                  <tr>
                    <th>상점 ID</th>
                    <th>사업자 번호</th>
                    <th>상호</th>
                    <th>사업구분</th>
                    <th>대표자</th>
                    <th>주소</th>
                    <th>상태</th>
                  </tr>
                  </thead>
                </table>
              </div>
            </div>
          </div>
        </div>
        <div class="row d-block d-lg-none">
          <div class="col-12">
            <div class="card">
              <div class="card-header border-0">
                <h3 class="card-title">상점 관리</h3>
              </div>
              <div class="card-body">
                <table id="dataTable1" class="table table-bordered table-hover">
                  <thead>
                  <tr>
                    <th>상점 목록</th>
                  </tr>
                  </thead>

                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
  </div>
  <!-- Modal -->
  <%- include('realTimeDeliveryModal') %>
  <footer class="main-footer">
    <strong>Copyright &copy; 2020 Bikeda.</strong>All rights reserved.
  </footer>
</div>
<!-- ./wrapper -->
<%- include('footer') %>
<script src="/js/branch/store.js"></script>
</body>
</html>
